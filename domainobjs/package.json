{
  "name": "maci-domainobjs",
  "version": "2.0.0-alpha",
  "description": "",
  "main": "build/ts/index.js",
  "files": [
    "build",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "types": "tsc -p tsconfig.json --noEmit",
    "test": "nyc ts-mocha --exit ts/__tests__/**.test.ts",
    "docs": "typedoc --plugin typedoc-plugin-markdown --options ./typedoc.json"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.8",
    "chai": "^4.3.10",
    "mocha": "^10.6.0",
    "nyc": "^17.0.0",
    "ts-mocha": "^10.0.0"
  },
  "dependencies": {
    "maci-crypto": "2.0.0-alpha"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "extensions": [
      ".ts"
    ],
    "all": true,
    "exclude": [
      "**/__tests__/*.ts",
      "**/*.js",
      "**/*.d.ts",
      "ts/index.ts"
    ],
    "branches": ">50%",
    "lines": ">50%",
    "functions": ">50%",
    "statements": ">50%"
  }
}
