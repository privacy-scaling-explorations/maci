{
  "name": "@maci-protocol/domainobjs",
  "version": "3.0.0",
  "private": false,
  "description": "Classes representing domain objects for MACI",
  "main": "build/ts/index.js",
  "files": [
    "build",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc -p tsconfig.build.json",
    "types": "tsc -p tsconfig.json --noEmit",
    "test": "nyc ts-mocha --exit ts/__tests__/**.test.ts",
    "docs": "typedoc --plugin typedoc-plugin-markdown --options ./typedoc.json"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "chai": "^4.3.10",
    "mocha": "^11.4.0",
    "nyc": "^17.1.0",
    "ts-mocha": "^11.1.0"
  },
  "dependencies": {
    "@maci-protocol/crypto": "^3.0.0"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "extensions": [
      ".ts"
    ],
    "all": true,
    "exclude": [
      "**/__tests__/*.ts",
      "**/*.js",
      "**/*.d.ts",
      "ts/index.ts"
    ],
    "branches": ">50%",
    "lines": ">50%",
    "functions": ">50%",
    "statements": ">50%"
  }
}
